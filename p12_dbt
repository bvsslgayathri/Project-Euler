
#include <stdio.h>
int check(long long int);
int main()
{
    long long int i,s=0,k;
    for(i=1;;i++)
    {
        s=(i+1)*i/2;
        k=check(s);
        if(k==1)
        break;
        else
        printf("%lld ",s);
        
    }
    printf("%lld ",s);
}
int check(long long int n)
{
    long long int i,c=0;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        c++;
    }
    if(c>=500)
    return 1;
    else
    {
        printf("%lld ",c);
    return 0;
    }
}
